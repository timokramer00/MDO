function get_airfoil(Au,Al)
%UNTITLED Summary of this function goes here
%   Detailed explanation goes here
X_vect = linspace(0,1,99)'; 
[Xtu,Xtl,C,Thu,Thl,Cm] = D_airfoil2(Au,Al,X_vect);

Xtu = flipud(Xtu);
Xtu(length(Xtu))=[];

fid = fopen('airfoil.dat', 'wt');
for j = 1:length(Xtu(:,1))
    fprintf(fid, '%f %f\n', Xtu(j,1),Xtu(j,1));
end
for i = 1:length(Xtl(:,1))
    fprintf(fid, '%f %f\n', Xtl(i,),Xtl(2,i));
end
fclose(fid);
